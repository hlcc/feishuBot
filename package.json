{
  "name": "openclaw-channel-feishu",
  "version": "1.0.0",
  "type": "module",
  "main": "./index.ts",
  "exports": {
    ".": "./index.ts"
  },
  "description": "Feishu (Lark) messaging channel plugin for OpenClaw",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "keywords": [
    "feishu",
    "lark",
    "openclaw",
    "openclaw-channel",
    "chatbot",
    "ai"
  ],
  "openclaw": {
    "extensions": ["./index.ts"],
    "channel": {
      "id": "feishu",
      "label": "Feishu (Lark)",
      "blurb": "Connect via Feishu Bot with WebSocket long connection"
    }
  },
  "scripts": {
    "setup": "npx ts-node --esm src/cli.ts setup",
    "status": "npx ts-node --esm src/cli.ts status",
    "uninstall": "npx ts-node --esm src/cli.ts uninstall"
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.35.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
