  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.35.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "json5": "^2.2.3"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "openclaw": "^2026.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
