{
  "name": "openclaw-channel-feishu",
  "version": "1.0.0",
  "description": "Feishu (Lark) messaging channel plugin for OpenClaw",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openclaw-channel-feishu": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "setup": "npm run build && node dist/cli.js setup",
    "status": "npm run build && node dist/cli.js status",
    "uninstall": "node dist/cli.js uninstall",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "feishu",
    "lark",
    "openclaw",
    "openclaw-channel",
    "chatbot",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "openclaw": {
    "extensions": ["./dist/index.js"],
    "channel": {
      "id": "feishu",
      "label": "Feishu (Lark)",
      "blurb": "Connect via Feishu Bot with WebSocket long connection"
    },
    "install": {
      "hints": [
        "Create a Feishu app at https://open.feishu.cn/app",
        "Enable 'im:message' and 'im:message:send' permissions",
        "Use WebSocket mode for event subscription"
      ]
    }
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.35.0",
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  }
}
